<h1> Editing course </h1>

<%= form_for :course, url: semester_course_path(@semester, @course), method: :patch do |f| %>
  <p>
    <%= f.label :subject %><br>
    <%= f.text_field :subject %>
  </p>
  <p>
    <%= f.label :number %><br>
    <%= f.number_field :number %>
  </p>
  <p>
    <%= f.label :credits %><br>
    <%= f.number_field :credits %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>


<%= link_to 'Back', semester_path(@semester) %>