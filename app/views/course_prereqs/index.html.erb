<p id="notice"><%= notice %></p>

<h1>Course Prereqs</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Prereq</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_prereqs.each do |course_prereq| %>
      <tr>
        <td><%= course_prereq.course %></td>
        <td><%= course_prereq.prereq %></td>
        <td><%= link_to 'Show', course_prereq %></td>
        <td><%= link_to 'Edit', edit_course_prereq_path(course_prereq) %></td>
        <td><%= link_to 'Destroy', course_prereq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Prereq', new_course_prereq_path %>
