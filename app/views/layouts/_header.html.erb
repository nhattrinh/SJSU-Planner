<header class= "navbar navbar-fixed-top navbar-inverse">
    <div class="container">
        <%= link_to "SJSU Graduation Planner", '#', id: "logo" %>
        <nav>
            <ul class="nav navbar-nav navbar-right">
                <% if logged_in? %>
                <li><%= link_to "Transcript", Grade %></li>
                <li><%= link_to "Courses", Course %></li>
                <li><%= link_to "Semesters", Semester %></li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><%= acting_student.full_name %><b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><%= link_to acting_user.email, acting_student %></li>
                        <li class="divider"></li>
                        <li><%= link_to "Logout", logout_path, method: :delete %></li>
                    </ul>
                </li>
            <% else %>
                <li><%= link_to "Login", login_path %></li>
            <% end %>
        </ul>
    </nav>
</div>
</header>
